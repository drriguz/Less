            "position": "bottom",
            "styles": [
                "assets/foundation/js/vendor/what-input.min.js"
            ]
        },
        "skins.less.js":  {
            "position": "bottom",
            "styles": [
                "assets/foundation/js/vendor/foundation.min.js"
            ],
            "dependencies": [
                "jquery.cookie",
                "skins.less.whatinput"
            ]
        }
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Less"
	}
