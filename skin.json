{
	"name": "Less",
	"author": "Riguz Lee",
	"url": "https://github.com/drriguz/less",
	"descriptionmsg": "less-desc",
	"namemsg": "skinname-less",
	"license-name": "GPL-2.0+",
    "requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"type": "skin",
	"ValidSkinNames": {
		"less": "Less"
	},
	"AutoloadClasses": {
